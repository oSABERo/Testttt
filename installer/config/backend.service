[Unit]
Description=Backend FastAPI Service
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/var/www/Testttt/apiii
Environment="PATH=/var/www/Testttt/venv/bin"
ExecStart=/var/www/Testttt/venv/bin/uvicorn --workers 3 -k uvicorn.workers.UvicornWorker --bind unix:Testttt/backend.sock -m 007 apiii.main:app
Restart=always

[Install]
WantedBy=multi-user.target
